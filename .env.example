# Conduit Benchmark Environment Variables
# Copy to .env and fill in your values

# =============================================================================
# LLM Provider API Keys (required for benchmarking)
# =============================================================================
# These are used by both the benchmark harness and the Conduit router

# OpenAI (gpt-4o, gpt-4o-mini, o1, o1-mini)
OPENAI_API_KEY=

# Anthropic (claude-sonnet-4, claude-opus-4, claude-haiku-4)
ANTHROPIC_API_KEY=

# Google/Gemini (gemini-2.0-flash, gemini-2.5-pro, gemini-3-pro-preview)
# Used for synthetic dataset generation
GOOGLE_API_KEY=

# Groq (llama-3.1-70b, llama-3.1-8b, mixtral-8x7b)
GROQ_API_KEY=

# Cohere (command-r-plus, command-r)
COHERE_API_KEY=

# HuggingFace (for embeddings)
HF_TOKEN=

# =============================================================================
# Database (PostgreSQL)
# =============================================================================
# Used by both benchmark harness and Conduit
DATABASE_URL=postgresql://postgres:password@localhost:5432/conduit_bench

# =============================================================================
# Redis (for Conduit caching)
# =============================================================================
REDIS_URL=redis://localhost:6379

# =============================================================================
# Benchmark-Specific Configuration
# =============================================================================

# Quality context for routing decisions
CONDUIT_QUALITY_CONTEXT=general

# =============================================================================
# Conduit Router Configuration (inherited from conduit-router)
# =============================================================================
# See conduit/.env.example for full list of supported variables.
# Common ones:

# Embedding provider (auto, openai, cohere, fastembed, sentence-transformers)
EMBEDDING_PROVIDER=openai

# Logging
LOG_LEVEL=INFO
ENVIRONMENT=development
